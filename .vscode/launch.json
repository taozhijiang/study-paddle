{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "clang++ start_simple_lr",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/inference_by_cpp/start_simple_lr/build/bin/start_simple_lr",
            "args": ["${workspaceFolder}/python_scripts/train_parameters"],
            "stopAtEntry": true,
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "logging": {
                "moduleLoad": false,
                "engineLogging": false,
                "trace": false
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "cwd": "${workspaceFolder}",
        }
    ]
}